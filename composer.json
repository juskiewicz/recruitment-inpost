{
    "name": "juskiewicz/recruitment-inpost",
    "description": "",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Tomasz JuÅ›kiewicz",
            "email": "to.juskiewicz@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/yaml": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/serializer": "6.4.*",
        "munusphp/munus": "^0.11.0",
        "symfony/property-access": "6.4.*",
        "symfony/form": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/twig-bundle": "6.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "audit": {
            "abandoned": "fail"
        },
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpunit:unit": [
            "phpunit --colors --testsuite unit"
        ],
        "tests": [
            "@phpunit:unit"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
